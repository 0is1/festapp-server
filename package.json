{
  "name": "festapp-server",
  "version": "0.0.0",
  "description": "Basic mockup server for festapp",
  "main": "server.js",
  "scripts": {
    "test": "node_modules/.bin/jshint server.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/futurice/festapp-server.git",
    "postinstall": "bower install",
    "update-webdriver": "webdriver-manager update",
    "protractor": "protractor test/protractor-conf.js",
    "test-single-run": "karma start test/karma.conf.js  --single-run",

    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + cat('lib/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'admin/index-async.html');\""
  


  },
  "keywords": [
    "festival",
    "backend"
  ],
  "author": "Juho Vähä-Herttua, Matias Kirvelä",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/futurice/festapp-server/issues"
  },
  "homepage": "https://github.com/futurice/festapp-server",
  "engines": {
    "node": "0.10.x"
  },
  "dependencies": {
    "express": ">= 3.5.0"
  },
  "devDependencies": {
    "jshint": "~2.5.0",
    "karma": "~0.10",
    "protractor": "~0.17.0",
    "http-server": "^0.6.1",
    "bower": "^1.3.1",
    "shelljs": "^0.2.6"
  }
}
